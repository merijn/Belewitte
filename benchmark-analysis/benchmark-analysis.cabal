Library
  Default-Language:     Haskell2010
  GHC-Options:          -O2 -Wall -fno-warn-unused-do-bind
  GHC-Prof-Options:     -fprof-auto
  Hs-Source-Dirs:       src
  Exposed-Modules:      Core
                        Model
                        OptionParsers
                        Query
                        Schema
                        Types
                        Utils

  C-Sources:            cbits/sqlite-functions.c
  CC-Options:           -Wall -Wextra -pedantic -std=c11 -g
  Include-Dirs:         cbits
  Includes:             sqlite-functions.h

  Build-Depends:        base
               ,        bytestring
               ,        binary >= 0.8.4
               ,        conduit == 1.3.*
               ,        containers == 0.5.*
               ,        data-interval == 1.3.*
               ,        exceptions == 0.10.*
               ,        haskeline == 0.7.4.2
               ,        interpolate == 0.1.1
               ,        megaparsec == 6.5.*
               ,        microlens == 0.4.8.*
               ,        monad-logger == 0.3.28.*
               ,        mtl == 2.2.1
               ,        optparse-applicative == 0.14.0.*
               ,        persistent == 2.8.*
               ,        persistent-template == 2.5.*
               ,        persistent-sqlite == 2.8.*
               ,        resourcet == 1.2.*
               ,        resource-pool
               ,        text
               ,        time == 1.9
               ,        transformers
               ,        unliftio-core == 0.1.1.*
               ,        vector == 0.12.0.1

