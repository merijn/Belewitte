    gpuId GPUId
    variantId VariantId
    stepId Int
    implId ImplementationId
    name Text
    minTime Double
    avgTime Double
    maxTime Double
    stdDev Double
    Primary gpuId variantId stepId implId name
    deriving Eq Show
